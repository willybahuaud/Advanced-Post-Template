{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wabeo/advanced-post-template",
  "title": "Advanced Post Template",
  "textdomain": "advanced-post-template",
  "description": "Variante du Post Template pour n'afficher qu'une tranche des r√©sultats d'une Query Loop.",
  "category": "theme",
  "ancestor": [ "core/query" ],
  "usesContext": [
    "queryId",
    "query",
    "displayLayout",
    "templateSlug",
    "previewPostType",
    "enhancedPagination",
    "postType"
  ],
  "attributes": {
    "startFrom": {
      "type": "number",
      "default": 1
    },
    "showCount": {
      "type": "number",
      "default": 0
    },
    "skipLast": {
      "type": "number",
      "default": 0
    }
  },
  "supports": {
    "reusable": false,
    "html": false,
    "align": ["wide", "full"],
    "layout": true,
    "color": {
      "gradients": true,
      "link": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": {
        "__experimentalDefault": "1.25em"
      }
    },
    "interactivity": {
      "clientNavigation": true
    }
  },
  "style": "wp-block-post-template",
  "editorStyle": "wp-block-post-template-editor",
  "editorScript": "file:./index.js"
}

